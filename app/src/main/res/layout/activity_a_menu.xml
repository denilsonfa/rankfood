<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".A_A_Menu"
    android:orientation="vertical"
    android:theme="@android:style/Theme.NoTitleBar">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="@color/cor_laranja"
        android:minHeight="?attr/actionBarSize"
        tools:ignore="UselessParent">

        <ImageView
            android:id="@+id/id_ic_register_back"
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:layout_gravity="center|start"
            android:layout_marginStart="20dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="2dp"
            android:background="@drawable/stl_item_incolor"
            android:clickable="true"
            android:contentDescription="@string/back"
            android:focusable="true"
            android:gravity="center"
            android:src="@drawable/ic_logo_branco"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:fontFamily="@font/ubuntu_bolder"
            android:text="@string/app_name"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="20dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:textAlignment="textStart"
            android:textAllCaps="true"
            android:textColor="@color/textBranco01"
            android:textSize="28sp"
            android:gravity="center"
            android:layout_gravity="center|start"
            tools:ignore="RtlCompat,TextContrastCheck"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="100"
        android:orientation="vertical"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="40dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="25"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <LinearLayout
                android:id="@+id/id_btn_menu_m01CreateRank"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/stl_btn_color"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:weightSum="100"
                android:clickable="true"
                android:focusable="true">

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center|start"
                    android:layout_margin="5dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:contentDescription="@string/back"
                    android:gravity="center"
                    android:src="@drawable/ic_icon_add"
                    tools:ignore="ImageContrastCheck"
                    android:layout_weight="30" />

                <TextView
                    android:text="@string/id_btn_menu_m01CreateRank"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/ubuntu_bolder"
                    android:layout_margin="5dp"
                    android:textAlignment="textStart"
                    android:textAllCaps="true"
                    android:textColor="@color/textBranco01"
                    android:textSize="24sp"
                    android:gravity="center"
                    android:layout_gravity="center|end"
                    tools:ignore="RtlCompat,TextContrastCheck"
                    android:layout_weight="70"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="25"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <LinearLayout
                android:id="@+id/id_btn_menu_m02Vote"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/stl_btn_color"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:weightSum="100"
                android:clickable="true"
                android:focusable="true">

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center|start"
                    android:layout_margin="5dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:contentDescription="@string/back"
                    android:gravity="center"
                    android:src="@drawable/ic_icon_star"
                    tools:ignore="ImageContrastCheck"
                    android:layout_weight="30" />

                <TextView
                    android:text="@string/id_btn_menu_m02Vote"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/ubuntu_bolder"
                    android:layout_margin="5dp"
                    android:textAlignment="textStart"
                    android:textAllCaps="true"
                    android:textColor="@color/textBranco01"
                    android:textSize="24sp"
                    android:gravity="center"
                    android:layout_gravity="center|end"
                    tools:ignore="RtlCompat,TextContrastCheck"
                    android:layout_weight="70"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="25"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <LinearLayout
                android:id="@+id/id_btn_menu_m03ViewRank"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/stl_btn_color"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:weightSum="100"
                android:clickable="true"
                android:focusable="true">

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center|start"
                    android:layout_margin="5dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:contentDescription="@string/back"
                    android:gravity="center"
                    android:src="@drawable/ic_icon_rank"
                    tools:ignore="ImageContrastCheck"
                    android:layout_weight="30" />

                <TextView
                    android:text="@string/id_btn_menu_m03ViewRank"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/ubuntu_bolder"
                    android:layout_margin="5dp"
                    android:textAlignment="textStart"
                    android:textAllCaps="true"
                    android:textColor="@color/textBranco01"
                    android:textSize="24sp"
                    android:gravity="center"
                    android:layout_gravity="center|end"
                    tools:ignore="RtlCompat,TextContrastCheck"
                    android:layout_weight="70"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="25"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <LinearLayout
                android:id="@+id/id_btn_menu_m04Sobre"
                style="@style/Widget.AppCompat.Button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/stl_btn_color"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:weightSum="100"
                android:clickable="true"
                android:focusable="true">

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center|start"
                    android:layout_margin="5dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:contentDescription="@string/back"
                    android:gravity="center"
                    android:src="@drawable/ic_icon_info"
                    tools:ignore="ImageContrastCheck"
                    android:layout_weight="30" />

                <TextView
                    android:text="@string/id_btn_menu_m04Sobre"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/ubuntu_bolder"
                    android:layout_margin="5dp"
                    android:textAlignment="textStart"
                    android:textAllCaps="true"
                    android:textColor="@color/textBranco01"
                    android:textSize="24sp"
                    android:gravity="center"
                    android:layout_gravity="center|end"
                    tools:ignore="RtlCompat,TextContrastCheck"
                    android:layout_weight="70"/>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>